{
  "expo": {
    "name": "TransPo",
    "slug": "transpo",
    "scheme": "transpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-maps",
        {
          "apiKey": "$EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
